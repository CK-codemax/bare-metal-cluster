# AWS region
region = "eu-north-1"

# Availability zones (comma-separated list)
zones = "eu-north-1a,eu-north-1b,eu-north-1c"

# Number of worker nodes
node_count = 3

# Worker node instance type
node_size = "t3.medium"

# Control plane (master) instance type
control_plane_size = "t3.medium"

# EBS volume size (in GB)
volume_size = 20
